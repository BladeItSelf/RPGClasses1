public class Player
{
    private string _name;
    public string Name
    {
        get { return _name; }
        set { _name = value; }
    }

    private int _health;
    public int Health
    {
        get { return _health; }
        set { _health = value; }
    }
    

    public Player(string name, int health)
    {
        Name = name;
    }
    
}